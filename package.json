{
  "name": "@nodescript/adapter-sql",
  "version": "0.0.1",
  "description": "SQL Adapter",
  "type": "module",
  "sideEffects": false,
  "private": true,
  "workspaces": [
    "protocol"
  ],
  "scripts": {
    "clean": "run-p clean:*",
    "clean:self": "rm -rf out *.tsbuildinfo",
    "clean:workspaces": "npm run clean --workspaces --if-present",
    "dev": "npm run compile && run-p dev:*",
    "dev:ts": "tsc -b -w",
    "dev:http": "NODE_ENV=development node ./out/bin/http.js",
    "compile": "npm run clean && tsc -b",
    "build": "run-s compile",
    "lint": "eslint --ext=.js,.ts,.vue --cache .",
    "test": "NODE_ENV=test mocha --no-silent --exit"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nodescript/eslint-config": "^1.2.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.5.5",
    "@types/pg": "^8.11.10",
    "eslint": "^8.57.0",
    "mocha": "^10.7.3",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@nodescript/adapter-sql-protocol": "^0.0.1",
    "@nodescript/errors": "^1.2.0",
    "@nodescript/http-server": "^2.6.0",
    "@nodescript/logger": "^2.0.6",
    "@nodescript/metrics": "^1.5.0",
    "@nodescript/microframework": "^1.12.0",
    "@nodescript/protocomm": "^1.1.0",
    "airtight": "^5.7.2",
    "dotenv": "^16.4.5",
    "mesh-config": "^1.1.2",
    "mesh-ioc": "^3.3.1",
    "mysql2": "^3.11.3",
    "nanoevent": "^1.0.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.1.14"
  }
}
